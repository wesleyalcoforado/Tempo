{
  "name": "Tempo",
  "version": "2.0",
  "description": "Shows the weather of your selected city.",
  "permissions": [
    "http://where.yahooapis.com/*",
    "http://weather.yahooapis.com/*",
    "geolocation"
  ],
  "icons": {
  	"16":  "tempo16.png",
  	"48":  "tempo48.png",
  	"128":  "tempo128.png"  	
  },
  "browser_action": {
    "default_icon": "tempo16.png"
  },
  "background": {
    "scripts": ["moduloj/jquery.min.js", "moduloj/bazo.js"]
  },
  "options_page": "options.html",
  "default_locale": "en",
  "manifest_version": 2
}
